/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "paths": {
      "oasis-button": [
        "./dist/oasis-button"
      ],
      "oasis-button-expand": [
        "./dist/oasis-button-expand"
      ],
      "oasis-calendar": [
        "./dist/oasis-calendar"
      ],
      "oasis-check-box": [
        "./dist/oasis-check-box"
      ],
      "oasis-combo-box": [
        "./dist/oasis-combo-box"
      ],
      "oasis-digit-input": [
        "./dist/oasis-digit-input"
      ],
      "oasis-drop-down-list": [
        "./dist/oasis-drop-down-list"
      ],
      "oasis-hint-box": [
        "./dist/oasis-hint-box"
      ],
      "oasis-loader": [
        "./dist/oasis-loader"
      ],
      "oasis-modal-window": [
        "./dist/oasis-modal-window"
      ],
      "oasis-tabs": [
        "./dist/oasis-tabs"
      ]
    },
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
